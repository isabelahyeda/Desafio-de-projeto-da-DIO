// Seção de Declarações 
var
contador:inteiro
limite:inteiro
resultado:inteiro
tabuada: inteiro

inicio

limite<-10

escreva("Qual tabuada você quer que eu resolva?:")
leia(tabuada)

para contador de 0 ate limite faca

resultado<- tabuada*contador
escreval(tabuada, " x",contador, " =" ,resultado)


fimpara







fimalgoritmo
